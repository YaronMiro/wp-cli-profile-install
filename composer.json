{
  "name": "YaronMiro/wp-profile",
  "description": "WP-CLI Command for installing a WordPress site from a profile via Yaml config files.",
  "homepage": "https://github.com/YaronMiro/wp-profile",
  "license": "MIT",
  "authors": [
    {
      "name": "Yaron Miro",
      "homepage": "https://github.com/YaronMiro",
      "email": "yaron.miro@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "autoload": {
    "classmap": [
      "command.php"
    ],
    "psr-4": {
      "YaronMiro\\WpProfile\\": "src/"
    }
  },
  "require": {
    "php": ">=5.3.9",
    "symfony/yaml": "2.8.9"
  },
  "require-dev": {
    "behat/behat": "~2.5"
  },
  "extras": {
  "commands": [
    "profile-install db",
    "profile-install site",
    "profile-install plugins",
    "profile-install themes"
    ]
  }
}
