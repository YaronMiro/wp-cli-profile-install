{
  "name": "YaronMiro/wp-cli-profile-installer",
  "description": "WP-CLI Command for installing a WordPress site from a profile.",
  "homepage": "https://github.com/YaronMiro/wp-cli-profile-installer",
  "license": "MIT",
  "authors": [
    {
      "name": "Yaron Miro",
      "homepage": "https://github.com/YaronMiro",
      "email": "yaron.miro@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "autoload": {
    "files": [ "command.php" ]
  },
  "require": {
    "php": ">=5.3.9",
    "symfony/yaml": "2.8.9"
  },
  "require-dev": {
    "behat/behat": "~2.5"
  },
  "extras": {
  "commands": [
    "profile-installer db",
    "profile-installer site",
    "profile-installer plugins",
    "profile-installer themes"
    ]
  }
}
